  "devDependencies": {
    "@now/build-utils": "0.11.1",
    "@now/node": "1.2.1",
    "@semantic-release/git": "7.0.18",
    "@types/dotenv": "6.1.1",
    "@types/lodash": "4.14.146",
    "@types/luxon": "1.15.2",
    "@types/node": "12.12.7",
    "@types/pino": "5.8.13",
    "@typescript-eslint/eslint-plugin": "2.11.0",
